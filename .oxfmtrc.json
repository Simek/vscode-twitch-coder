{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "always",
  "endOfLine": "crlf",
  "printWidth": 120,
  "bracketSpacing": true,
  "ignorePatterns": [
    ".idea",
    ".vscode-test",
    "node_modules",
    "out",
    "tests-out",
    "*.vsix"
  ]
}
